#include<stdio.h>
int main()
{

int num1,num2;

  printf(" enter num1:",num1);
  scanf("%d",&num1);

  printf(" enter num2:",num2);
  scanf("%d",&num2);

  
  printf("addition of no1 and no2 is : %d", no1+no2);
  printf("subtraction of no1 and no2 is : %d", no1-no2);
  printf("multiplication of no1 and no2 is : %d", no1*no2);
  printf("division of no1 and no2 is : %d", no1/no2);

  return 0;
}
